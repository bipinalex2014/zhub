<table>
    <thead>
        <tr>
            <th>No</th>
            <th>Name</th>
            <th>Options</th>
        </tr>
    </thead>
    <tbody>
        {{#each buildings}}
        <tr>
            <td>{{this.no}}</td>
            <td>{{this.name}}</td>
            <td><button>Options</button></td>
        </tr>
        {{/each}}
    </tbody>
</table>